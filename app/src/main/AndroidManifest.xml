<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fimebookandmarket">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home.Configuracion"></activity>
        <activity android:name=".Home.Perfil" />
        <activity android:name=".Home.Libros" />
        <activity android:name=".Home.Menu" />
        <activity android:name=".Home.Home" />
        <activity android:name=".Inicio.SignIn" />
        <activity android:name=".Inicio.Register" />
        <activity android:name=".Inicio.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>